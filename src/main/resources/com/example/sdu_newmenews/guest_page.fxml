                  <Label prefHeight="80.0" prefWidth="900.0" style="-fx-text-fill: #564557;" text="New cars sold in EU must be zero-emission from 2035" textAlignment="JUSTIFY" wrapText="true">
                     <font>
                        <Font name="Montserrat ExtraBold" size="30.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="300.0" fitWidth="774.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true">
